{
    "version": 2,
    "name": "node-js",
    "builds": [
      {
        "src": "server/package.json",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm install && chmod +x node_modules/.bin/prisma && prisma generate"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)", "dest": "/"
      }
    ]
  }
  